    <aside class="main-sidebar">
        <section class="sidebar">
            <ul class="sidebar-menu" data-widget="tree">
                {#                <li class="header">MAIN NAVIGATION</li>#}
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-users"></i> <span>Производители</span>
                        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="/admin/vendor"><i class="fa fa-list"></i> Список</a></li>
                    </ul>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-folder"></i>
                        <span>Категории</span>
                        <span class="pull-right-container">
                             <i class="fa fa-angle-left pull-right"></i>
            </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="/admin/folder"><i class="fa fa-list"></i> Список</a></li>
                    </ul>
                </li>
                <li class="treeview">
                    <a href="#">
                        <i class="fa fa-coffee"></i>
                        <span>Товары</span>
                        <span class="pull-right-container">
                             <i class="fa fa-angle-left pull-right"></i>
            </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="/admin/product"><i class="fa fa-list"></i> Список</a></li>
                    </ul>
                </li>
            </ul>
        </section>
    </aside>
    <div class="content-wrapper">
        <section class="content-header">
            <h1>
                {% block title %}CoffeeTime Admin{% endblock %}
                <small> {% block title_small %}{% endblock %}</small>
            </h1>
        </section>
        <section class="content">
            {% block content %}{% endblock %}
        </section>
    </div>
    <footer class="main-footer">
        <div class="pull-right hidden-xs">
            <b>Version</b> {{ app.version }}
        </div>
        <strong>Copyright &copy; 2020 <a href="http://trimere.site">fenK</a>.</strong> All rights reserved.
    </footer>
    <div class="control-sidebar-bg"></div>
</div>

<!-- jQuery 3 -->
<script src="/assets/plugins/adminLTE/bower_components/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap 3.3.7 -->
<script src="/assets/plugins/adminLTE/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<script src="/assets/plugins/adminLTE/dist/js/adminlte.min.js"></script>
